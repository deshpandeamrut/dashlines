<project>
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.dashlines.app</groupId>
	<artifactId>dashlines-news-app</artifactId>
	<version>1</version>
	<parent>
		<groupId>org.springframework.boot</groupId>
		<artifactId>spring-boot-starter-parent</artifactId>
		<version>2.0.5.RELEASE</version>
	</parent>
	<packaging>pom</packaging>


	<properties>
		<java.version>1.8</java.version>
		<maven.compiler.version>3.6.1</maven.compiler.version>
		<maven.resources.plugin.version>3.0.2</maven.resources.plugin.version>
		<maven.bundle.plugin.version>3.0.0</maven.bundle.plugin.version>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<commons.lang.version>3.4</commons.lang.version>
		<lombok.version>1.16.20</lombok.version>
		<spring-cloud.version>Finchley.BUILD-SNAPSHOT</spring-cloud.version>
	</properties>


	<build>
		<plugins>
			<plugin>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-maven-plugin</artifactId>
			</plugin>
		</plugins>
	</build>

	<modules>
		<module>dashlines-news-app</module>
		<module>dashlines-web-gateway</module>
		<module>dashlines-eureka-server</module>

	</modules>
</project>